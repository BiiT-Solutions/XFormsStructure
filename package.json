{
  "name": "x-forms",
  "version": "0.1.23",
  "scripts": {
    "ng": "ng",
    "start": "npm run prepare:translations && ng serve",
    "build": "ng build",
    "build:library": "ng build x-forms-lib",
    "prepare:docs": "cpx ./README.md ./dist/x-forms && echo \"{\\\"name\\\": \\\"`node -e \"console.log(require('./package.json').name);\"\n`\\\", \\\"version\\\": \\\"`node -e \"console.log(require('./package.json').version);\"\n`\\\"}\" > ./dist/x-forms/package.json",
    "prepare:publication": "npm run build && npm run prepare:docs && npm run prepare:translations",
    "clean:translations": "del-cli --force ./src/assets/i18n/biit-ui/",
    "prepare:translations": "npm run clean:translations && cpx './node_modules/biit-ui/assets/i18n/**/*.json' './src/assets/i18n/biit-ui/'",
    "publish": "npm run prepare:publication && npm publish ./dist/x-forms",
    "publish:library": "npm run build:library && npm run prepare:translations && npm publish ./dist/x-forms-lib",
    "publish:all": "npm run publish && npm run publish:library",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "v15-lts",
    "@angular/common": "v15-lts",
    "@angular/compiler": "v15-lts",
    "@angular/core": "v15-lts",
    "@angular/forms": "v15-lts",
    "@angular/platform-browser": "v15-lts",
    "@angular/platform-browser-dynamic": "v15-lts",
    "@angular/router": "v15-lts",
    "@ngneat/transloco-locale": "^5.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "v15-lts",
    "@angular/cli": "v15-lts",
    "@angular/compiler-cli": "v15-lts",
    "@types/jasmine": "~4.3.0",
    "biit-icons-collection": "latest",
    "biit-ui": "latest",
    "cpx": "^1.5.0",
    "del-cli": "^5.1.0",
    "jasmine-core": "~4.5.0",
    "@ngneat/transloco": "^4.2.2",
    "@ngneat/transloco-scoped-libs": "^5.0.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "~15.2.2",
    "typescript": "~4.9.4"
  }
}
